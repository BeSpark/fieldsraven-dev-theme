<div class="xfield">
  <label for="start">My most important date is:</label>
  <input type="date" id="start" name="customer-dob" x-model="custtomAttributes.mostImportantDate">
</div>
<fieldset>
  <legend>What are you interested in?</legend>
  <input type="radio" id="html" name="fav_language" value="HTML" x-model="custtomAttributes.intrest">
  <label for="html">HTML</label><br>
  <input type="radio" id="css" name="fav_language" value="CSS" x-model="custtomAttributes.intrest">
  <label for="css">CSS</label><br>
  <input type="radio" id="javascript" name="fav_language" value="JavaScript" x-model="custtomAttributes.intrest">
  <label for="javascript">JavaScript</label>
</fieldset>
